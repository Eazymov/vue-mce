{
  "name": "vue-mce",
  "version": "1.3.5",
  "description": "Best VueJS component for TinyMCE",
  "main": "dist/vue-mce.common.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "maintainers": [
    {
      "name": "Eazymov",
      "email": "eazymovcode@gmail.com"
    }
  ],
  "scripts": {
    "dev": "webpack --config config/common.config.js --watch",
    "build": "npm run build:web && npm run build:common && npm run build:test",
    "build:web": "webpack --config config/web.config.js",
    "build:common": "webpack --config config/common.config.js",
    "build:test": "webpack --config config/test.config.js",
    "test": "npm run test:types && npm run test:unit && npm run test:e2e",
    "test:types": "tsc -p test/types/tsconfig.json",
    "test:unit": "babel-node test/unit/runner.js",
    "test:e2e": "babel-node test/e2e/runner.js"
  },
  "registry": "github",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eazymov/vue-mce.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "tinymce",
    "component",
    "eazymov",
    "javascript"
  ],
  "author": "Eduard Troshin <eazymovcode@gmail.com>",
  "license": "MIT",
  "readme": "",
  "bugs": {
    "url": "https://github.com/Eazymov/vue-mce/issues"
  },
  "homepage": "https://github.com/Eazymov/vue-mce#readme",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.1",
    "chromedriver": "^2.33.2",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nightwatch": "^0.9.16",
    "node-sass": "^4.6.0",
    "sass-loader": "^6.0.6",
    "selenium-server": "^3.7.0",
    "selenium-standalone": "^6.11.0",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vue": "^2.5.3",
    "vue-class-component": "^6.1.0",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "peerDependencies": {
    "vue": "^2.5.3"
  }
}
